{
  "extends": "airbnb",
  "globals": {
    "MAIN_WINDOW_WEBPACK_ENTRY": true,
    "SPLASH_SCREEN_WEBPACK_ENTRY": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "react/jsx-sort-props": [2],
    "import/extensions": "off",
    "react/prop-types": "off"
  }
}